{
    "name": "Poop Recipes",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Turns your recipe websites into something... crappy.",
    
    "icons": { 
        "16": "Icon/Icon16.png",
        "64": "Icon/Icon64.png",
        "128": "Icon/Icon128.png"
    },
    
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "run_at": "document_start",
        "js": [
            "PoopRecipes.js"
        ]
    }, {
        "matches": [
            "http://allrecipes.com/*",
            "https://allrecipes.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/AllRecipes.js"
        ]
    }, {
        "matches": [
            "http://www.foodnetwork.com/*",
            "https://www.foodnetwork.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/FoodNetwork.js"
        ]
    }, {
        "matches": [
            "http://www.thekitchn.com/*",
            "https://www.thekitchn.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/TheKitchn.js"
        ]
    }, {
        "matches": [
            "http://www.yummly.com/*",
            "https://www.yummly.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/Yummly.js"
        ]
    }, {
        "matches": [
            "http://www.chow.com/*",
            "https://www.chow.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/Chow.js"
        ]
    }, {
        "matches": [
            "http://www.epicurious.com/*",
            "https://www.epicurious.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/Epicurious.js"
        ]
    }, {
        "matches": [
            "http://www.simplyrecipes.com/*",
            "https://www.simplyrecipes.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/SimplyRecipes.js"
        ]
    }, {
        "matches": [
            "http://www.myrecipes.com/*",
            "https://www.myrecipes.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/MyRecipes.js"
        ]
    }, {
        "matches": [
            "http://www.eatingwell.com/*",
            "https://www.eatingwell.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/EatingWell.js"
        ]
    }, {
        "matches": [
            "http://www.cooks.com/*",
            "https://www.cooks.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/Cooks.js"
        ]
    }, {
        "matches": [
            "http://www.kraftrecipes.com/*",
            "https://www.kraftrecipes.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "Sites/KraftRecipes.js"
        ]
    }],

    "permissions": [
        "<all_urls>"
    ]
}